        <script type="text/javascript">
        // Wait for the DOM to be ready
        $(function() {
          // Initialize form validation on the registration form.
          // It has the name attribute "registration"
          $("form[name='registration']").validate({
            // Specify validation rules
            rules: {
              // The key name on the left side is the name attribute
              // of an input field. Validation rules are defined
              // on the right side
              textName: "required",
              textComment: "required",
              // youare: "required",
              textEmail: {
                required: true,
                // Specify that email should be validated
                // by the built-in "email" rule
                email: true
              },
              textNumber: {
                required: true,
                minlength: 10,
                maxlength: 10
              }
            },
            // Specify validation error messages
            messages: {
              textName: "Please enter your name",
              textComment: "Please enter your subject",
              selectQuery: "Please select an query",
              // youare: "Please select you are",
              textNumber: {
                required: "Please provide a mobile no.",
                minlength: "Your mobile must be at least 10 digit long",
                minlength: "Your mobile must be at least 10 digit long"
              },
              textEmail: "Please enter a valid email address"
            },
            // Make sure the form is submitted to the destination defined
            // in the "action" attribute of the form when valid
            submitHandler: function(form) {
              form.submit();
            }
          });
        });
        </script>
        
